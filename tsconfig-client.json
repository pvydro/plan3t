{
  "compilerOptions": {
    "outDir": "lib",
    "module": "commonjs",
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "jsx": "react",
    "target": "es5",
    "declaration": true,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "src/network/client/**/*.ts",
  ]
}